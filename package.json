{
    "name": "react-ssr",
    "version": "1.0.0",
    "description": "Server side rendering project",
    "main": "index.js",
    "scripts": {
        "test": "node testConfig/scripts/test.js --env=jsdom",
        "test-watch": "npm test -- --watch",
        "start-loc": "webpack-dev-server --hot --inline",
        "build-prod": "npm run dev",
        "dev": "MODE=development npm-run-all --parallel dev:*",
        "dev:build-server": "webpack --config webpack.server-dev.js --watch",
        "dev:build-client": "webpack --config webpack.client-dev.js --watch",
        "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
        "build-sw": "sw-precache --root='public/' --static-file-globs='public/**/!(*map*)'",
        "prod": "MODE=production webpack --config webpack.server.js && webpack --config webpack.client.js && sw-precache --root='publicTemp/' --static-file-globs='publicTemp/**/!(*map*)' && rm -rf public/static && cp -r publicTemp/* public/ && rm -r build/ && cp -r buildTemp/ build/",
        "prod-server": "MODE=production nodemon --watch build --exec \"node build/bundle.*.js\""
    },
    "author": "Rishabh Rawat",
    "license": "ISC",
    "dependencies": {
        "autoprefixer": "^9.3.1",
        "axios": "^0.18.0",
        "babel-cli": "6.26.0",
        "babel-loader": "7.1.4",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-es2017": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "compression": "1.7.3",
        "concurrently": "4.1.0",
        "express": "4.16.4",
        "express-http-proxy": "1.5.0",
        "lodash": "4.17.11",
        "nodemon": "1.18.7",
        "npm-run-all": "4.1.5",
        "postcss-flexbugs-fixes": "^4.1.0",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "react-helmet": "5.2.0",
        "react-icons": "^3.1.0",
        "react-loadable": "^5.4.0",
        "react-redux": "5.1.1",
        "react-router": "^4.3.1",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "4.3.1",
        "redux": "4.0.1",
        "redux-thunk": "2.3.0",
        "serialize-javascript": "1.5.0",
        "webpack": "4.26.1",
